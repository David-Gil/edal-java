<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:Image xmlns:ns2="http://www.resc.reading.ac.uk">
    <RasterLayer>
        <DataFieldName>TMP</DataFieldName>
        <PaletteColourScheme>
            <ScaleRange>
                <ScaleMin>270.0</ScaleMin>
                <ScaleMax>310.0</ScaleMax>
                <Logarithmic>false</Logarithmic>
            </ScaleRange>
            <ColourMap>
                <MissingDataColour>#00000000</MissingDataColour>
                <NumberOfColourBands>10</NumberOfColourBands>
                <Palette>redblue</Palette>
            </ColourMap>
        </PaletteColourScheme>
    </RasterLayer>
    <ContourLayer>
        <DataFieldName>TMP</DataFieldName>
        <AutoscaleEnabled>true</AutoscaleEnabled>
        <NumberOfContours>10.0</NumberOfContours>
        <ContourLineColour>#FF000000</ContourLineColour>
        <ContourLineWidth>1</ContourLineWidth>
        <ContourLineStyle>DASHED</ContourLineStyle>
        <LabelEnabled>true</LabelEnabled>
    </ContourLayer>
</ns2:Image>