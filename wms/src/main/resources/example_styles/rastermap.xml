<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- An example of using a mapped colour scheme in a raster layer -->
<StyledLayerDescriptor version="1.1.0" xsi:schemaLocation="http://www.opengis.net/sld
StyledLayerDescriptor.xsd" xmlns="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc"
xmlns:se="http://www.opengis.net/se" xmlns:xlink="http://www.w3.org/1999/xlink"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:resc="http://www.resc.reading.ac.uk">
  <NamedLayer>
    <se:Name>testlayer</se:Name>
    <UserStyle>
      <se:CoverageStyle>
        <se:Rule>
          <se:RasterSymbolizer>
            <se:Opacity>1.0</se:Opacity>
            <se:ColorMap>
              <resc:Map fallbackValue="#000000">
                <se:Value dataValue="1" dataLabel="Vegetation">#FF0000FF</se:Value>
                <se:Value dataValue="2" dataLabel="Mud">#FF00FFFF</se:Value>
                <se:Value dataValue="3" dataLabel="Concrete">#FF00FF00</se:Value>
                <se:Value dataValue="4" dataLabel="Water">#FFFF00FF</se:Value>
                <se:Value dataValue="5" dataLabel="Swamp">#FFFF0000</se:Value>
                <se:Value dataValue="6" dataLabel="People">#aaffff00</se:Value>
              </resc:Map>
            </se:ColorMap>
          </se:RasterSymbolizer>
        </se:Rule>
      </se:CoverageStyle>
    </UserStyle>
  </NamedLayer>
</StyledLayerDescriptor>
