<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<resc:Image xmlns:resc='http://www.resc.reading.ac.uk'
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'>
    <ContourLayer>
        <DataFieldName>$layerName</DataFieldName>
        <Scale>
            <ScaleMin>$scaleMin</ScaleMin>
            <ScaleMax>$scaleMax</ScaleMax>
        </Scale>
        <AutoscaleEnabled>false</AutoscaleEnabled>
        <NumberOfContours>10.0</NumberOfContours>
        <ContourLineColour>#FF000000</ContourLineColour>
        <ContourLineWidth>1</ContourLineWidth>
        <ContourLineStyle>DASHED</ContourLineStyle>
        <LabelEnabled>true</LabelEnabled>
    </ContourLayer>
</resc:Image>    