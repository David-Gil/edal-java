xmlns:se="http://www.opengis.net/se" xmlns:xlink="http://www.w3.org/1999/xlink"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:resc="http://www.resc.reading.ac.uk">
  <UserLayer>
    <se:Name>OSTIA/analysed_sst_confidence_intervals</se:Name>
    <LayerCoverageConstraints>
      <CoverageConstraint>
        <se:CoverageName>OSTIA/sst_lb</se:CoverageName>
      </CoverageConstraint>
      <CoverageConstraint>
        <se:CoverageName>OSTIA/sst_ub</se:CoverageName>
